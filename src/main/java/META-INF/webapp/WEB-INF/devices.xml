<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:util="http://www.springframework.org/schema/util" xmlns:jpa="http://www.springframework.org/schema/data/jpa"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd
           http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-3.0.xsd
           http://www.springframework.org/schema/data/jpa http://www.springframework.org/schema/data/jpa/spring-jpa-1.1.xsd">
	
	<bean id="lighting" class="net.phyokyaw.jaquapi.dao.model.Device">
		<constructor-arg value="Lighting" />
	</bean>
	
	<bean id="pump" class="net.phyokyaw.jaquapi.dao.model.Device">
		<constructor-arg value="Return Pump" />
	</bean>
	
	<bean id="skimmer" class="net.phyokyaw.jaquapi.dao.model.Device">
		<constructor-arg value="Skimmer" />
	</bean>
	
	<bean id="refulight" class="net.phyokyaw.jaquapi.dao.model.Device">
		<constructor-arg value="Refugium Light" />
	</bean>
	
	<bean id="heater" class="net.phyokyaw.jaquapi.dao.model.Device">
		<constructor-arg value="Heater" />
	</bean>

	<bean id="i2cDevices" class="java.util.ArrayList">
	    <constructor-arg>
	        <list>
	            <ref bean="lighting" />
	            <ref bean="pump" />
	            <ref bean="skimmer" />
	            <ref bean="refulight" />
	            <ref bean="heater" />
	        </list>
	    </constructor-arg>
	</bean>
	
	<bean id="deviceScheduleLoader" class="net.phyokyaw.jaquapi.dao.model.DeviceScheduleLoader">
		<property name="lighting"></property>
	</bean>
</beans>